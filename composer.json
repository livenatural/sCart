{
    "name": "livenatural/s_cart",
    "description": "Simple Laravel Shopping Cart",
    "type": "metapackage",
    "license": "MIT",
    "require": {
        "illuminate/support": "5.8.*",
        "illuminate/session": "5.8.*",
        "illuminate/events": "5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.2",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.8"
    },
    "autoload": {
        "psr-4": {
            "Livenatural\\SCart\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Livenatural\\SCart\\SCartServiceProvider"
            ],
            "aliases": {
                "SCart": "Livenatural\\SCart\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "dev"
}
